Procedure special_page_PGDOWN {

    for (i=1; {i}<11; i={i}+1) {
        Compareto "{web_name}\{web_name}{j}.png" method="search2"
        if ({_EXIT_CODE} > 0) {
            Press "PGDOWN" wait="500"
        } else {
            Click "image" passrate="50" template="{web_name}\{web_name}{j}.png" wait="500"
